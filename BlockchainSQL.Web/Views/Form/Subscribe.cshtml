@model BlockchainSQL.Web.Models.SubscribeFormInput
@using (Html.BeginAweFormEx(Model)) {
    <div class="input-group">
        @Html.TextBoxFor(o => o.Email, new { @class = "form-control" })
        <span class="input-group-btn">
            <input type="submit" value="Send" class="btn btn-ar btn-lg btn-default" />
        </span>
    </div>
}


