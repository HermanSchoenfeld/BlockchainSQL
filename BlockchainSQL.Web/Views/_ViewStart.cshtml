@using BlockchainSQL.Web.Models
@using Hydrogen.Data
@{
    Layout = "~/Views/Shared/_Layout.cshtml";

    if (this.ViewContext.ViewBag.FunLinks == null) {
        ViewContext.ViewBag.FunLinks = new FunLink[0];
    }

    // defaults
    ViewContext.ViewBag.EnableSyntaxHighlighting = false;
    ViewContext.ViewBag.SQLDialect = DBMSType.SQLServer;
    ViewContext.ViewBag.ShowLoading = true;
}
