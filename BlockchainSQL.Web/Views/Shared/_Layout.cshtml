    <link href="@Url.Content("~/Content/themes/gui2/AwesomeMvc.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/gui2/mods.css")" rel="stylesheet" type="text/css" />    
    <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
    <script type="text/javascript" src="/Scripts/app.js"></script>
