    <link href="@Url.Content("/Content/css/width-full.css")" rel="stylesheet">
    <link href="@Url.Content("/Content/css/app.css")" rel="stylesheet"/>
    @if (ViewBag.EnableSyntaxHighlighting) {
        <link href="@Url.Content("/Content/css/codemirror.css")" rel="stylesheet"/>
    }

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="/Scripts/html5shiv.min.js"></script>
