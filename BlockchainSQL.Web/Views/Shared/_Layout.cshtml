@using BlockchainSQL.Web.Controllers
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="/images/favicon.ico"/>
    <meta name="description" content="">

    <!-- CSS -->
    <link href="/css/preload.css" rel="stylesheet">
    <link href="@Url.Content("/css/vendors.css")" rel="stylesheet">
    <link href="@Url.Content("/css/style-blue.css")" rel="stylesheet">
    <link href="@Url.Content("/css/width-full.css")" rel="stylesheet">
    <link href="@Url.Content("/css/app.css")" rel="stylesheet"/>
    @if (ViewBag.EnableSyntaxHighlighting) {
        <link href="@Url.Content("/css/codemirror.css")" rel="stylesheet"/>
    }

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="/Scripts/html5shiv.min.js"></script>
        <script src="/Scripts/respond.min.js"></script>
    <![endif]-->

    <link href="@Url.Content("~/themes/gui2/AwesomeMvc.css")" rel="stylesheet" type="text/css"/>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script type="text/javascript" src="/Scripts/app.js"></script>
    @if (ViewBag.EnableSyntaxHighlighting) {
        <script src="@Url.Content("~/Scripts/syntax/codemirror.js")"></script>
        <script src="@Url.Content("~/Scripts/syntax/sql.js")"></script>
    }
</head>

@if (ViewBag.ShowLoading) {
    <!-- Preloader -->
    <div id="preloader">
        <div id="status">&nbsp;</div>
    </div>
}

<body>
@Html.Partial("_Analytics")
<div id="sb-site">
    <div class="boxed">
        @Html.Partial("_Header")
        @Html.Partial("_Menu")
        @if (ViewData.ContainsKey("PageMessages")) {
            @Html.Partial("_PageMessages", (IEnumerable<PageMessage>)ViewBag.PageMessages)
        }
        @RenderBody()
        @Html.Partial("_Footer")
    </div> <!-- boxed -->
</div> <!-- sb-site -->
@Html.Partial("_SlideBarRight")
@Html.Partial("_BackTopButton")

<script src="@Url.Content("~/Scripts/vendors.js")"></script>
<script src="@Url.Content("~/Scripts/app_init.js")"></script>
<script src="@Url.Content("~/Scripts/AwesomeMvc.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/utils.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/awem.js")" type="text/javascript"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.13.0/jquery.validate.min.js" type="text/javascript"></script>
<script src="https://ajax.aspnetcdn.com/ajax/mvc/5.1/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
<script type="text/javascript" src="https://code.jquery.com/ui/1.11.3/jquery-ui.min.js"></script>

@Html.Awe().Init();

</body>

</html>